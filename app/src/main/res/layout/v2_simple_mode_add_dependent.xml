<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_gradient">


    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="92dp"
        android:background="#2e276c"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="79dp"
        android:layout_height="92dp"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logov2_" />


    <TextView
        android:id="@+id/textView46"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserratbold"
        android:text="ADD DEPENDENT"
        android:textColor="#FFFFFF"
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/frequency_txt2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="84dp"
        android:fontFamily="@font/montserrat"
        android:text="Enter Dependents' Email Address"
        android:textColor="#000000"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />


    <ImageView
        android:id="@+id/helpAddDependent"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="86dp"
        app:layout_constraintStart_toEndOf="@+id/frequency_txt2"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"
        app:srcCompat="@drawable/question" />

    <EditText
        android:id="@+id/dependentEmail"
        android:layout_width="249dp"
        android:layout_height="46dp"
        android:ems="10"
        android:fontFamily="@font/montserrat"
        android:hint="Email"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="@+id/frequency_set_later_bs"
        app:layout_constraintEnd_toEndOf="@+id/frequency_set_later_bs"
        app:layout_constraintHorizontal_bias="0.529"
        app:layout_constraintStart_toStartOf="@+id/frequency_set_later_bs"
        app:layout_constraintTop_toTopOf="@+id/frequency_set_later_bs"
        app:layout_constraintVertical_bias="0.789" />

    <Spinner
        android:id="@+id/frequency_set_later_bs8"
        android:layout_width="283dp"
        android:layout_height="65dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/edittext3"
        android:backgroundTint="#2e276c"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView18" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="263dp"
        android:layout_height="55dp"
        android:fontFamily="@font/montserrat"
        android:outlineSpotShadowColor="#2E276C"

        app:layout_constraintBottom_toBottomOf="@+id/frequency_set_later_bs8"
        app:layout_constraintEnd_toEndOf="@+id/frequency_set_later_bs8"
        app:layout_constraintHorizontal_bias="0.55"
        app:layout_constraintStart_toStartOf="@+id/frequency_set_later_bs8"
        app:layout_constraintTop_toTopOf="@+id/frequency_set_later_bs8"
        app:layout_constraintVertical_bias="0.7" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="96dp"
        android:fontFamily="@font/montserrat"
        android:text="Send Report Weekly"
        android:textColor="#000000"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frequency_set_later_bs" />

    <Spinner
        android:id="@+id/frequency_set_later_bs"
        android:layout_width="283dp"
        android:layout_height="65dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/edittext3"
        android:backgroundTint="#2e276c"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frequency_txt2" />

    <Button
        android:id="@+id/save_bs_later"
        android:layout_width="247dp"
        android:layout_height="49dp"
        android:layout_marginTop="68dp"
        android:background="@drawable/edit_round"
        android:fontFamily="@font/montserrat"
        android:text="SUBMIT"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#FFFFFF"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner" />

    <Button
        android:id="@+id/cancel_bs_later"
        android:layout_width="247dp"
        android:layout_height="49dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/edittext3"
        android:fontFamily="@font/montserrat"
        android:onClick="laterbs_To_lhm"
        android:text="CANCEL"
        android:textAppearance="@style/TextAppearance.AppCompat.Large.Inverse"
        android:textColor="#2e276c"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/save_bs_later" />

</androidx.constraintlayout.widget.ConstraintLayout>